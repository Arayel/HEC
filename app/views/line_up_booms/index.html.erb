<h1>Listing line_up_booms</h1>

<table>
  <thead>
    <tr>
      <th>Djname</th>
      <th>Settime</th>
      <th>Musicstyle</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_up_booms.each do |line_up_boom| %>
      <tr>
        <td><%= line_up_boom.DjName %></td>
        <td><%= line_up_boom.SetTime %></td>
        <td><%= line_up_boom.MusicStyle %></td>
        <td><%= link_to 'Show', line_up_boom %></td>
        <td><%= link_to 'Edit', edit_line_up_boom_path(line_up_boom) %></td>
        <td><%= link_to 'Destroy', line_up_boom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line up boom', new_line_up_boom_path %>
